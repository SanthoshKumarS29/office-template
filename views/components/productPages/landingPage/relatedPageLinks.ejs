<section class="rpl-section" id="related-services">
    <div class="py-5">
        <div class="flex-y gap-5">
            <div class="text-center">
                <h2><%= page.relatedPage.title %></h2>
                <p><%= page.relatedPage.shortDes %></p>
            </div>
            <div class="row justify-content-center g-3">
                <% page.relatedPage.links.forEach(cont => { %>
                    <div class="col-xl-4">
                        <a href="<%= cont.link %>" class="rpl-link-container">
                            <div class="flex-x justify-content-between">
                                <p><%= cont.linkTxt %></p>
                                <svg width="24" height="24" viewBox="0 0 34 34" fill="none"
                                    xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                    <rect width="34" height="34" fill="url(#pattern0_80_3)" />
                                    <defs>
                                        <pattern id="pattern0_80_3" patternContentUnits="objectBoundingBox" width="1"
                                            height="1">
                                            <use xlink:href="#image0_80_3" transform="scale(0.01)" />
                                        </pattern>
                                        <image id="image0_80_3" width="100" height="100" preserveAspectRatio="none"
                                            xlink:href="data:image/png;base64," />
                                    </defs>
                                </svg>
                            </div>
                            <div>
                                <p><%= cont.linkContent %></p>
                            </div>
                        </a>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
</section>