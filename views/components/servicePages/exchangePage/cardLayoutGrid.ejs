<section>
    <div class="py-5">
        <div class="text-center pb-4">
            <h2><%- page.cardLayout.title %></h2>
        </div>
        <div class="custom-overflow-scroll">
            <div class="row justify-content-center gx-2 gy-3">
                <% page.cardLayout.cards.forEach(card => { %>
                    <div class="col-md-6 col-xl-3">
                        <div class="cardDesign h-100">
                            <strong class="fs-4 fw-medium blue-clr"><%= card.headtxt %></strong>
                            <p class="text-normal line-hs"><%- card.headdes %></p>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
</section>