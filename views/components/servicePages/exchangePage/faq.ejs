<section>
    <div class="py-5">
        <div class="text-center">
            <h2><%- page.faq.title %></h2>
        </div>
        <div class="pt-4 col-lg-10 mx-auto">
            <div class="accordion" id="faqAccordion">
                <% page.faq.qnsAns.forEach((qnaItems, i) => { %>
                <div class="accordion-item">
                    <h2 class="accordion-header" id="heading<%= i %>">
                        <button class="accordion-button text-normal-sm <%= i !== 0 ? 'collapsed' : '' %>" type="button" data-bs-toggle="collapse"
                            data-bs-target="#collapse<%= i %>" aria-expanded="<%= i === 0 ? 'true' : 'false' %>" aria-controls="collapse<%= i %>">
                            <%= qnaItems.qns %>
                        </button>
                    </h2>
                    <div id="collapse<%= i %>" class="accordion-collapse collapse <%= i === 0 ? 'show' : '' %>" data-bs-parent="#faqAccordion">
                        <div class="accordion-body">
                            <p class="text-normal"><%= qnaItems.ans %></p>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</section>