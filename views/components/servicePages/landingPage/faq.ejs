<section>
    <div class="py-5">
        <div class="row justify-content-center g-4">
            <div class="col-xl-6">
                <div class="flex-y gap-3">
                    <div class="flex-y gap-1">
                        <p class="fs-4"><%= page.faq.subTitle %></p>
                        <h2 class="display-6"><%= page.faq.title %></h2>
                    </div>
                    <div class="accordion" id="faqAccordion">
                        <% page.faq.qnsAns.forEach((qnaItems, i)=> { %>
                            <div class="accordion-item">
                                <h3 class="accordion-header" id="heading<%= i %>">
                                    <button class="accordion-button text-normal-sm <%= i !== 0 ? 'collapsed' : '' %>"
                                        type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%= i %>"
                                        aria-expanded="<%= i === 0 ? 'true' : 'false' %>"
                                        aria-controls="collapse<%= i %>">
                                        <%= qnaItems.qns %>
                                    </button>
                                </h3>
                                <div id="collapse<%= i %>"
                                    class="accordion-collapse collapse <%= i === 0 ? 'show' : '' %>"
                                    data-bs-parent="#faqAccordion">
                                    <div class="accordion-body">
                                        <p class="text-normal">
                                            <%= qnaItems.ans %>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <% }) %>
                    </div>
                </div>
            </div>
            <div class="col-xl-6">
                <div class="faq-cta">
                    <h3 class="mb-0"><%= page.faq.ctaHead %></h3>
                    <p class="fs-6"><%= page.faq.ctaDes %></p>
                    <a href="/contact-us" class="btn btn-primary"><%= page.faq.ctaBtnTxt %></a>
                </div>
            </div>
        </div>
    </div>
</section>